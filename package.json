{
  "name": "prolly",
  "version": "0.5.1",
  "description": "A minimalist utility library for ES6 Native Promises",
  "main": "index.js",
  "scripts": {
    "test": "npm run mocha && npm run lint",
    "ci-test": "npm run coverage && npm run lint && ./node_modules/.bin/codecov",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec",
    "mocha": "mocha -R spec ./test",
    "lint": "./node_modules/eslint/bin/eslint.js ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growombud/prolly.git"
  },
  "keywords": [
    "promise",
    "es6",
    "native",
    "utility",
    "sequence",
    "series",
    "minimalist",
    "small"
  ],
  "author": "Jonathan Griggs <jonathan.griggs@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/growombud/prolly/issues"
  },
  "readmeFilename": "README.md",
  "homepage": "https://github.com/growombud/prolly#readme",
  "devDependencies": {
    "codecov": "3.1.0",
    "eslint": "6.2.2",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-mocha": "5.2.0",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "should": "13.2.3"
  },
  "engine": "node >= 4.0.0"
}